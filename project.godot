; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Claim"
run/main_scene="res://ui/main_ui.tscn"
config/features=PackedStringArray("4.6", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

EventBus="*res://core/autoloads/event_bus.gd"
GameState="*res://core/autoloads/game_state.gd"
Simulation="*res://core/autoloads/simulation.gd"
TimeScale="*res://core/autoloads/time_scale.gd"
HTTPFetcher="*res://core/services/http_fetcher.gd"
EphemerisVerifier="*res://core/services/ephemeris_verifier.gd"

[display]

window/size/viewport_width=680
window/size/viewport_height=1200
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[physics]

3d/physics_engine="Jolt Physics"

[rendering]

rendering_device/driver.windows="d3d12"
