;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Claim"
config/features=PackedStringArray("4.6", "Forward Plus")
config/icon="res://icon.svg"
run/main_scene="res://ui/main_ui.tscn"

[autoload]

EventBus="*res://core/autoloads/event_bus.gd"
GameState="*res://core/autoloads/game_state.gd"
